{
  "base": {
    "dataset": {
      "source": "synthetic",
      "name": "stripe_scale",
      "dim": 2,
      "synthetic": {
        "generator": "stripe_ctrl_alpha",
        "n_r": 50000,
        "n_s": 50000,
        "alpha": 1.0,
        "seed": 1,
        "params": {
          "control_axis": 1,
          "core_lo": 0.45,
          "core_hi": 0.55,
          "gap_factor": 0.1,
          "delta_factor": 0.25,
          "shuffle_strips": true,
          "shuffle_r": false,
          "swap_sides": false
        }
      }
    },
    "run": {
      "method": "ours",
      "variant": "sampling",
      "t": 100000,
      "seed": 1,
      "repeats": 3,
      "j_star": 1000000,
      "enum_cap": 0,
      "write_samples": false,
      "verify": false,
      "extra": {
        "pbsm_scheme": "stripes",
        "pbsm_k": 0,
        "pbsm_part_axis": 0,
        "pbsm_sweep": "orthogonal",
        "tlsop_nx": 128,
        "tlsop_ny": 128,
        "tlsop_reuse_sort": true,
        "sirs_outer": "",
        "sirs_leaf_size": 32,
        "rej_index": "S",
        "rej_rep_center": false,
        "rej_count_draws": 50000,
        "rej_max_factor": 10000
      }
    },
    "output": {
      "out_dir": "results/sweeps/scale",
      "run_tag": "scale_sweep"
    },
    "logging": {
      "level": "info",
      "with_timestamp": true,
      "with_thread_id": false
    },
    "sys": {
      "threads": 1
    }
  },
  "sweep": {
    "n_r": [
      50000,
      100000,
      200000,
      400000,
      800000
    ],
    "n_s": [
      50000,
      100000,
      200000,
      400000,
      800000
    ],
    "t": [
      100000
    ],
    "method": [
      "ours",
      "range_tree",
      "kd_tree"
    ],
    "variant": [
      "sampling",
      "adaptive"
    ]
  },
  "files": {
    "raw": "sweep_raw.csv",
    "summary": "sweep_summary.csv"
  },
  "meta": {
    "note": "Scale sweep varies n_r and n_s. Current sjs_sweep takes Cartesian product of n_r x n_s; filter results for the diagonal (n_r==n_s) if you want equal-scale only.\n\n[compat] This repo build supports method: ours | range_tree | kd_tree. Note: kd_tree only implements the 'sampling' variant; other variants will be reported as unsupported/skipped by sjs_sweep.",
    "unsupported_methods_removed_from_sweep_method": [
      "aabb",
      "interval_tree",
      "r_tree",
      "pbsm",
      "tlsop",
      "sirs",
      "rejection"
    ]
  }
}
